            <Button
                android:id="@+id/koyrapolice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:gravity="left"
                android:textStyle="bold"
                android:layout_below="@+id/fultola"
                android:drawableRight="@android:drawable/sym_action_call"
                android:text="Koyra police station"
                android:layout_marginTop="13dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />


            <Button
                android:id="@+id/fultolaPolice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:gravity="left"
                android:textStyle="bold"
                android:layout_below="@+id/fultola"
                android:drawableRight="@android:drawable/sym_action_call"
                android:text="Fultola Police Station"
                android:layout_marginTop="13dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />
        </TableLayout>

    </ScrollView>



            <Button
                android:id="@+id/batiaghatPolice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:gravity="left"
                android:textStyle="bold"
                android:drawableRight="@android:drawable/sym_action_call"
                android:text="Batiaghata police station"
                android:layout_marginTop="13dp"
                android:layout_below="@+id/gazihospi"
                android:layout_alignParentStart="true" />

            <Button
                android:id="@+id/Horintana"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:gravity="left"
                android:textStyle="bold"
                android:layout_marginTop="13dp"
                android:drawableRight="@android:drawable/sym_action_call"
                android:text="Harintana police station"
                android:layout_below="@+id/AkijHospi"
                android:layout_alignParentStart="true" />

            <Button
                android:id="@+id/daulutpurPolice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
